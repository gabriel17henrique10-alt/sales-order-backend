{
  // Visit https://aka.ms/tsconfig to read more about this file
  "compilerOptions": {
      "target": "ES2024",
      "module": "Node16",
      "moduleResolution": "node16",
      "strict": true, 
      "strictNullChecks": true,
      "noImplicitAny": true, //this not permit an variable whithout type
      "esModuleInterop": true,
      "outDir": "./dist",
      "baseUrl": "./",
      "rootDir": "./",
      "paths": {
        "@models/*": ["./@cds-models/*/index.ts"]
      }
  },
  "include": ["srv"],
  "exclude": ["node_modules"]
}